{
	"name": "tfs-default-project-structure",
	"displayName": "TFS - Default Project Structure",
	"description": "Helpers to create a default project in React or React Native",
	"version": "0.0.5",
	"author": {
		"name": "Woods"
	},
	"galleryBanner": {
		"color": "#eff1f3",
		"theme": "light"
	},
  "publisher": "tfs-pub",
	"engines": {
		"vscode": "^1.42.0"
  },
	"categories": [
		"Other"
	],
	"icon": "src/images/ico.png",
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.createReactNativeDefaultPage",
				"title": "tfs-create-react-native-default-page"
			},
			{
				"command": "extension.createReactNativeProjectStructure",
				"title": "tfs-create-react-native-project-structure"
			}
		]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Woodsphreaker/tfs-vscode-plugin"
  },
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"babel-eslint": "^10.1.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-config-standard": "^14.1.0",
		"eslint-plugin-import": "^2.20.0",
		"eslint-plugin-node": "^11.0.0",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-standard": "^4.0.1",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
